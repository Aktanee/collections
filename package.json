{
  "name": "collections",
  "version": "0.0.1",
  "description": "Chrome extension for quick browse to a collection of link",
  "scripts": {
    "dev": "NODE_ENV=development webpack --watch",
    "build": "NODE_ENV=production webpack",
    "dist": "NODE_ENV=production webpack && node scripts/zip.js",
    "lint": "./node_modules/.bin/eslint src",
    "format": "npm run prettier -- --write",
    "validate": "npm run lint && npm run format",
    "prettier": "./node_modules/.bin/prettier \"src/**/*.+(js|vue|scss)\"",
    "precommit": "./node_modules/.bin/lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aktanee/collections.git"
  },
  "keywords": [],
  "author": "Antoine Renault <antoine.renault.mmi@gmail.com> (https://antoinerenault.fr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aktanee/collections/issues"
  },
  "homepage": "https://github.com/Aktanee/collections#readme",
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.5.4",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "jest": "^23.5.0",
    "jest-vue-preprocessor": "^1.4.0",
    "lint-staged": "^7.3.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.14.3",
    "puppeteer": "^1.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "superagent": "^3.8.3",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.22.0",
    "webpack-chrome-extension-reloader": "^0.8.3",
    "webpack-cli": "^3.1.0",
    "zip-folder": "^1.0.0"
  },
  "dependencies": {
    "vue": "^2.5.17"
  }
}
