{
  "name": "Collections",
  "version": "0.0.1",
  "description": "Chrome extension for quick browse to a collection of link",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "permissions": [
    "storage",
    "webNavigation",
    "tabs",
    "*://*/"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "images/get_started128.png",
    "default_title": "Collections",
    "default_popup": "index.html"
  }
}